<Page x:Class="moicursach.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:moicursach"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminPage">

    <Grid>
        <Button x:Name="Categories" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,450,0" Click="Categories_Click">Categories</Button>
        <Button x:Name="Prices" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0" Click="Prices_Click">Prices</Button>
        <Button x:Name="Products" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="450,0,0,0" Click="Products_Click">Products</Button>
    </Grid>
</Page>
